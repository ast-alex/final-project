{{#each posts}}
<div class="col-lg-4 col--6 col-12">
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">{{this.title}}</h5>
            <p class="card-text">{{this.body}}</p>
            <a href="/posts/{{this.slug}}" class="btn btn-primary">Ver mas...</a>
        </div>
    </div>
</div>    
{{/each}}